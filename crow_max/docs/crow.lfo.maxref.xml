<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./c74ref.xsl" type="text/xsl"?>

<c74object name="crow.lfo" module="crow">

	<digest>
		Generate CV LFOs on crow
	</digest>

	<description>
		Connect to the <o>crow</o> object to automatically generate CV LFOs on the crow eurorack module.
	</description>

	<!--METADATA-->
	<metadatalist>
		<metadata name="author">Sam Wolk</metadata>
		<metadata name="tag">crow</metadata>
		<metadata name="tag">crow help</metadata>
		<metadata name="tag">Eurorack</metadata>
	</metadatalist>

	<!--ARGUMENTS-->
	<objarglist>
		<objarg name="Output Index" optional="0" type="int">
            <digest>
				Hardware output to turn into LFO (default is 1)
			</digest>
			<description>
				This value cannot be changed at runtime.  Use one object per output if you want multiple LFOs.
			</description>
        </objarg>
		<objarg name="Period" optional="1" type="float">
            <digest>
				LFO period in seconds
			</digest>
        </objarg>
		<objarg name="Amplitude" optional="1" type="float">
            <digest>
				LFO amplitude in volts.
			</digest>
			<description>
				LFO amplitude in volts.  Negative values invert the LFO.
			</description>
        </objarg>
		<objarg name="Skew" optional="1" type="float">
            <digest>
				Expects 0.0-1.0; skews the LFO waveshape.
			</digest>
			<description>
				0 corresponds to Saw, 0.5 to tri, and 1.0 to Ramp
			</description>
        </objarg>
	</objarglist>

	<!--MESSAGES-->
	<methodlist>
		 
	</methodlist>

	<!--DISCUSSION-->
	<discussion>
	</discussion>

	<!--SEEALSO-->
	<seealsolist>
		<seealso name="crow"/>
		<seealso name="crow.volts"/>
		<seealso name="crow.ar"/>
		<seealso name="crow.adsr"/>
		<seealso name="crow.inputs"/>
		<seealso name="crow.function"/>
		<seealso name="crow.var"/>
		<seealso name="crow.makevar"/>
		<seealso name="crow.n2v"/>	
	</seealsolist>

	

</c74object>